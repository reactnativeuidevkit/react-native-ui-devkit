{
  "name": "react-native-ui-devkit",
  "version": "2.0.8",
  "description": "Native UI Development Kit for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "index.tsx",
    "src",
    "lib",
    "rn-ui-devkit-config.json",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "react-component",
    "react native",
    "react-native-ui-devkit",
    "react native ui devkit",
    "react-native interface",
    "react-native components",
    "react-native component",
    "react-native uikit",
    "react-native toolkit",
    "react-native toolset",
    "react-native theme",
    "react-native themed",
    "react-native styled",
    "rn-ui-devkit",
    "rn ui devkit",
    "rnuidevkit",
    "native-style",
    "elements",
    "rn elements",
    "react-native base",
    "rn base",
    "rn interface",
    "rn components",
    "rn component",
    "react-native ui",
    "ui",
    "rn ui",
    "rn devkit",
    "rn uikit",
    "rn toolkit",
    "rn toolset",
    "rn theme",
    "rn themed",
    "rn styled",
    "action",
    "button",
    "card",
    "checkbox",
    "collapsible",
    "flatlist",
    "icon",
    "radio",
    "swipeable",
    "textinput",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactnativeuidevkit/react-native-ui-devkit.git"
  },
  "author": "React Native UI DevKit <developer@reactnativeuidevkit.com> (https://reactnativeuidevkit.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactnativeuidevkit/react-native-ui-devkit/issues"
  },
  "homepage": "https://reactnativeuidevkit.com/",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.2.2",
    "@react-native/eslint-config": "^0.72.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^28.1.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "@types/react-native-vector-icons": "^6.4.14",
    "commitlint": "^17.0.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-builder-bob": "^0.20.0",
    "release-it": "^15.0.0",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "bundleDependencies": [
    "react-native-animatable",
    "moti"
  ],
  "dependencies": {
    "moti": "^0.26.0",
    "react-native-animatable": "^1.3.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "packageManager": "^yarn@1.22.15",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "declarationMap": false,
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "directories": {
    "lib": "lib"
  }
}
